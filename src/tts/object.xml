<Defaults>
    <Panel
        class="WebBuzzerPanel"
        width="100"
        height="100"
        position="0 0 -55">
    </Panel>

    <Image
        class="WebBuzzerQR"
        height="80"
        width="80"
        rectAlignment="UpperCenter"
        preserveAspect="true"
        offsetXY="0 -5">
    </Image>

    <InputField
        class="WebBuzzerLink"
        text=" "
        height="75"
        width="850"
        fontSize="50"
        rectAlignment="LowerCenter"
        scale="0.1 0.1 0.1"
        offsetXY="0 5"
        textAlignment="MiddleCenter"
        readOnly="true">
    </InputField>
</Defaults>

